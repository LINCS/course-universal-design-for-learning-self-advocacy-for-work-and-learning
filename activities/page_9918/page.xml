    <content>&lt;p dir="ltr" style="text-align: left;"&gt;&lt;/p&gt;&lt;div&gt;&lt;p xml:lang="EN-US" lang="EN-US" paraid="0" paraeid="{8a3bef37-02a1-44d7-8439-557c4619d529}{145}" data-ccp-props="{&amp;quot;335551550&amp;quot;:1,&amp;quot;335551620&amp;quot;:1,&amp;quot;335559683&amp;quot;:0,&amp;quot;335559685&amp;quot;:10,&amp;quot;335559731&amp;quot;:0,&amp;quot;335559737&amp;quot;:0,&amp;quot;335562764&amp;quot;:2,&amp;quot;335562765&amp;quot;:1.14,&amp;quot;335562766&amp;quot;:4,&amp;quot;335562767&amp;quot;:0,&amp;quot;335562768&amp;quot;:4,&amp;quot;335562769&amp;quot;:6}"&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;There are both external and internal barriers to learner agency.&amp;nbsp;&amp;nbsp;&lt;/span&gt;​&lt;/p&gt;&lt;/div&gt;&lt;div&gt;&lt;p xml:lang="EN-US" lang="EN-US" paraid="0" paraeid="{8a3bef37-02a1-44d7-8439-557c4619d529}{148}" data-ccp-props="{&amp;quot;335551550&amp;quot;:1,&amp;quot;335551620&amp;quot;:1,&amp;quot;335559683&amp;quot;:0,&amp;quot;335559685&amp;quot;:10,&amp;quot;335559731&amp;quot;:0,&amp;quot;335559737&amp;quot;:0,&amp;quot;335562764&amp;quot;:2,&amp;quot;335562765&amp;quot;:1.14,&amp;quot;335562766&amp;quot;:4,&amp;quot;335562767&amp;quot;:0,&amp;quot;335562768&amp;quot;:4,&amp;quot;335562769&amp;quot;:6}"&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;Examples of&amp;nbsp;&lt;/span&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;&lt;strong&gt;external barriers&lt;/strong&gt;&lt;/span&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;&lt;strong&gt;&amp;nbsp;&lt;/strong&gt;include:&lt;/span&gt;​&lt;/p&gt;&lt;/div&gt;&lt;ul role="list"&gt;&lt;li data-aria-posinset="1" role="listitem" data-aria-level="1"&gt;&lt;p xml:lang="EN-US" lang="EN-US" paraid="0" paraeid="{8a3bef37-02a1-44d7-8439-557c4619d529}{151}" data-ccp-props="{&amp;quot;335551550&amp;quot;:1